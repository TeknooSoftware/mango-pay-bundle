{
    "name": "teknoo/mango-pay-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to import and use the Mango Pay PHP SDK into a Symfony 2.7+ projects, and use it as standard Symfony services.",
    "keywords": ["mango pay","bundle", "mango"],
    "homepage": "http://teknoo.software/mango-pay",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "mangopay/php-sdk-v2": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.2",
        "squizlabs/php_codesniffer": "~3.0",
        "pdepend/pdepend": "~2.5",
        "phpmd/phpmd": "~2.6",
        "sebastian/phpcpd": "~3.0",
        "phploc/phploc": "~4.0",
        "doctrine/common": "<2.8",
        "doctrine/annotations": "<1.5",
        "doctrine/cache": "<1.7",
        "doctrine/collections": "<1.5",
        "doctrine/instantiator": "<1.1",
        "symfony/symfony": "^2.7.21||~3.0",
        "symfony/framework-bundle": "^2.7.9||~3.0",
        "symfony/form": "^2.7.9||~3.0",
        "symfony/finder": "^2.7.9||~3.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Teknoo\\MangoPayBundle\\": "src/Teknoo/MangoPayBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\MangoPayBundle\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
    }
}
