{
    "name": "unialteri/mango-pay-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to import and use the Mango Pay PHP SDK into a Symfony 2.7+ projects, and use it as standard Symfony services.",
    "keywords": ["mango pay","bundle", "mango"],
    "homepage": "http://teknoo.it/mango-pay",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "r.deloge@uni-alteri.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "mangopay/php-sdk-v2": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/symfony": "~2.7",
        "symfony/framework-bundle": "~2.7",
        "symfony/form": "~2.7",
        "symfony/finder": "~2.7",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2"
    },
    "autoload": {
        "psr-4": {
            "UniAlteri\\MangoPayBundle\\": "src/MangoPayBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UniAlteri\\MangoPayBundle\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
    }
}
